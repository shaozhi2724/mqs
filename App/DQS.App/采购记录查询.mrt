        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>采购记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT  
	p.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.CommonName AS [通用名称],
	p.ProductSpec AS [规格],
	p.PackageSpec AS [包装规格],
	p.ProductStyle AS [药品类别],
	p.PhysicType AS [剂型],
	p.AuthorizedNo AS [批准文号],
	p.ProducerName AS [生产厂商],
	p.StockCondition AS [储存库区],
	b.BillCode AS [采购订单编号],
	b.BillDate AS [购货日期],
	b.BillTypeName AS [订单类型],
	b.Operator AS [采购员],
	b.DealerName AS [供应商名称],
	b.DealerSpell AS [供应商Spell],
	b.BusinessPerson AS [供应商业务员],
	bd.UnitPrice AS [价格],
	bd.Amount AS [数量],
	bd.TotalPrice AS [金额]
	FROM BUS_Bill b
		INNER JOIN BUS_BillDetail bd
			ON b.BillID = bd.BillID
		INNER JOIN BFI_Product p
			ON bd.ProductID = p.ProductID
      WHERE b.isbillin = '1' AND BillTypeID = '1' 
		 And BillStatusName &lt;&gt;'已删除' 
 		 AND p.ProductName  LIKE '%{TextProductName.Text}%'--药品名称
	   	 AND b.DealerName   LIKE '%{TextDealerName.Text}%'--[供应商名称]
         AND b.BillCode     LIKE '%{TextBillCode.Text}%'--采购单号
		 AND (b.BillDate    BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextBillDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextBillDatetwo.Value)}')
	</SqlCommand>
      </采购记录>
