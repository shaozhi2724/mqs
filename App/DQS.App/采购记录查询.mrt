            var result = System.Text.RegularExpressions.Regex.Replace(d, ".", m =&gt; "负元空零壹贰叁肆伍陆柒捌玖空空空空空空空分角拾佰仟万亿兆京垓秭穰"[m.Value[0] - '-'].ToString());
