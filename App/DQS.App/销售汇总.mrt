      <Sale Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Sale</Alias>
        <Columns isList="true" count="4">
          <value>BillCode,System.String</value>
          <value>BillDate,System.DateTime</value>
          <value>DealerName,System.String</value>
          <value>TotalPrice,System.Double</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Sale</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT  
	b.BillCode ,
	b.BillDate ,
	b.DealerName ,
	CASE b.BillTypeName
        WHEN '销售出货' THEN b.TotalPrice
        ELSE -b.TotalPrice
    END AS TotalPrice
FROM dbo.BUS_Bill b
WHERE   BillTypeName IN ( '销售出货', '销售退货' )
		AND (b.BillDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",DTPCOne.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",DTPCTwo.Value)}')
        AND (b.DealerName LIKE '%{txtDealer.Text}%' OR b.DealerSpell LIKE '%{txtDealer.Text}%')
ORDER BY {IIF(CbcOrder.Text=="单据号","b.BillID,b.DealerID","b.DealerID,b.BillID").ToString()}</SqlCommand>
      </Sale>
