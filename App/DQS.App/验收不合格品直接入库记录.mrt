          <value>单位地址,System.String</value>
          <value>往来单位名称,System.String</value>
          <value>验收结果,System.String</value>
          <value>创建日期,System.DateTime</value>
          <value>批号,System.String</value>
          <value>生产日期,System.DateTime</value>
          <value>有效期,System.DateTime</value>
          <value>不合格数量,System.Int32</value>
          <value>不合格事项及处理措施,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Product</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT 
	bf.ProductName AS [药品名称],
	bf.ProductStyle AS [药品类别],
	bf.PackageSpec AS [包装规格],
	bf.PhysicType AS [剂型],
	bf.AuthorizedNo AS [批准文号],
	bf.ProducerName AS [生产厂商],
	ba.AcceptCode AS [验收单号],
	ba.BillCode AS [订单编号],
	ba.BillTypeName AS [订单类型],
	ff.DealerAddress AS [单位地址],
	ba.DealerName AS [往来单位名称],
	bd.AcceptResult AS [验收结果],
	ba.CreateDate AS [创建日期],
	bd.BatchNo AS [批号],
	bd.ProduceDate AS [生产日期],
	bd.ValidateDate AS [有效期],
	bd.UnqualifiedAmount [不合格数量],
	bd.DetailRemark AS [不合格事项及处理措施]
	FROM  BUS_Accept AS ba
		INNER JOIN BUS_AcceptDetail AS bd 
					ON bd.AcceptID = ba.AcceptID
		INNER JOIN BFI_Product AS bf
					ON bf.ProductID = bd.ProductID
		INNER  JOIN BFI_Dealer AS ff
					ON  ff.DealerID = ba.DealerID
	WHERE bd.UnqualifiedAmount &lt;&gt;0
                       and bf.ProductName  like   '%{txtProductName.Text}%' --药品名称
					   and bd.BatchNo      like   '%{txtBatchNo.Text}%' --批号
                       and ba.BillTypeName like   '%{txtBillTypeName.Text}%'--订单类型
					   AND (ba.CreateDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextCreateDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextCreateDatetwo.Value)}')</SqlCommand>
      </Product>
