      <Product Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Product</Alias>
        <Columns isList="true" count="16">
          <value>药品ID,System.Int32</value>
          <value>药品编号,System.String</value>
          <value>药品名称,System.String</value>
          <value>药品名称Spell,System.String</value>
          <value>规格,System.String</value>
          <value>生产厂商,System.String</value>
          <value>生产厂商Spell,System.String</value>
          <value>批准文号,System.String</value>
          <value>包装规格,System.String</value>
          <value>单位,System.String</value>
          <value>剂型,System.String</value>
          <value>药品类别,System.String</value>
          <value>销毁总量,System.Int32</value>
          <value>销毁时间,System.DateTime</value>
          <value>批号,System.String</value>
          <value>销毁单号,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Product</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>

SELECT     p.ProductID AS 药品ID, 
	p.ProductCode AS 药品编号, 
	p.ProductName AS 药品名称, 
	p.ProductSpell AS 药品名称Spell, 
	p.PackageSpec AS 规格, 
	p.ProducerName AS 生产厂商, 
	p.ProducerSpell AS 生产厂商Spell, 
	p.AuthorizedNo AS 批准文号, 
	p.PackageSpec AS 包装规格, 
	p.ProductUnit AS 单位, 
	p.PhysicType AS 剂型, 
	BP.DestroyDate AS 销毁时间,
	BP.DestroyCode AS 销毁单号,
	s.BatchNo AS  批号,
	p.ProductStyle AS 药品类别, SUM(s.Amount) AS 销毁总量
	FROM       dbo.BUS_ProdcutDestroyDetail AS s 
				INNER JOIN  dbo.BFI_Product AS p 
						ON s.ProductID = p.ProductID
				INNER JOIN dbo.BUS_ProdcutDestroy as BP
						ON s.DestroyID = BP.DestroyID
 	where	p.ProductName like '%{txtDealerName.Text}%' 
		AND	p.ProducerName like '%{txtProducerName.Text}%'
		AND	s.BatchNo like '%{txtBatchNo.Text}%'
		AND(BP.DestroyDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextDestroyDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextDestroyDatetwo.Value)}')
GROUP BY p.ProductID, p.ProductCode, p.ProductName, p.ProductSpell, p.PackageSpec, p.ProducerName, p.ProducerSpell, p.AuthorizedNo, p.PackageSpec, p.ProductUnit, p.PhysicType, p.ProductStyle,BP.DestroyDate,s.BatchNo,BP.DestroyCode</SqlCommand>
      </Product>
