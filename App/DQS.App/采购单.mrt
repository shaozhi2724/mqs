      <JH Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>JH</Alias>
        <Columns isList="true" count="7">
          <value>ProductName,System.String</value>
          <value>PackageSpec,System.String</value>
          <value>ProductUnit,System.String</value>
          <value>ProducerName,System.String</value>
          <value>Amount,System.Int32</value>
          <value>UnitPrice,System.Double</value>
          <value>TotalPrice,System.Double</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>JH</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="1">
          <value>BillID,,8,0</value>
        </Parameters>
        <SqlCommand>SELECT 
	p.ProductName,
	p.PackageSpec,
	p.ProductUnit,
	p.ProducerName,
	bd.Amount,
	bd.UnitPrice,
	bd.TotalPrice
	FROM dbo.BUS_BillDetail bd
	INNER JOIN dbo.BFI_Product p
		ON bd.ProductID = p.ProductID
WHERE bd.BillID = @BillID</SqlCommand>
      </JH>
      <JHT Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>JHT</Alias>
        <Columns isList="true" count="10">
          <value>DealerName,System.String</value>
          <value>BillDate,System.DateTime</value>
          <value>BillCode,System.String</value>
          <value>DealerAddress,System.String</value>
          <value>DealerPhone,System.String</value>
          <value>AccountBankName,System.String</value>
          <value>AccountNo,System.String</value>
          <value>CertificateNo,System.String</value>
          <value>TotalPrice,System.Double</value>
          <value>BigTotalPrice,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>JHT</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="1">
          <value>BillID,,8,0</value>
        </Parameters>
        <SqlCommand>SELECT 
	d.DealerName,
	CONVERT(DATE,b.BillDate,23) AS [BillDate],
	b.BillCode,
	d.DealerAddress,
	d.DealerPhone,
	d.AccountBankName,
	d.AccountNo,
	ISNULL((SELECT CertificateNo FROM dbo.BFI_Qualification 
		WHERE BelongTable = 'BFI_Dealer' 
			AND b.DealerID = BelongID 
			AND CertificateStyle = '税务登记证'),'') AS [CertificateNo],
	b.TotalPrice,
	dbo.usf_ChineseCost(b.TotalPrice) AS [BigTotalPrice]
FROM dbo.BUS_Bill b
	INNER JOIN dbo.BFI_Dealer d
		ON b.DealerID = d.DealerID
WHERE b.BillID = @BillID</SqlCommand>
      </JHT>
