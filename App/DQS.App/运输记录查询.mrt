        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>运输记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT 
	t.TransportID AS [运输ID],
	t.TransportCode AS [运输单号],
	t.DeliveryPerson AS [送货人],
	t.BillCode AS [订单编号],
	t.RelateCompany AS [收货单位],
	t.RelateAddress AS [收货地址],
	--t.RelatePerson AS [收货人],
	--t.RelatePersonContact AS [收货人联系方式],
	t.ShippingAddress AS [发货地址],
	t.CommissionedManagers AS [委托经办人],
	t.TransportType AS [运输方式],
	t.CarryCompnay AS [承运单位],
	t.CarryCompnaySpell AS [承运单位spell],
	t.CarCode AS [运输车辆号],
	t.StartTime AS [启运时间],
	t.ArriveTime AS [到达时间],
	t.TransportStatus AS [状态],
	t.MinTimeLimit AS [最低运输时限],
	u1.UserName AS [创建人],
	t.CreateDate AS [创建日期],
	t.TransportRemark AS [备注],
	t.CreateDate AS [发货时间],
	t.TransportStyle AS [运输工具],
	b.Amount AS [药品件数],
	t.TransportCode AS [货单号]	
	FROM BUS_TransportRecord t
		LEFT JOIN ATC_User u1 ON t.CreateUserID = u1.UserID
left JOIN BUS_BillDetail  b ON t.BillID = b.BillID		
WHERE       t.TransportCode  LIKE '%{TextTransportCode.Text}%' --运输单号
		 AND t.RelateCompany LIKE '%{TextRelateCompany.Text}%'--[收货单位]
		 AND (t.CreateDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextCreateDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextCreateDatetwo.Value)}')
	   	 and t.TransportType LIKE '%{TextTransportType.Text}%'--运输工具</SqlCommand>
      </运输记录>
