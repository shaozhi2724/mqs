        <Dictionary isRef="1" />
        <Name>销售记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT  
	p.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.CommonName AS [通用名称],
	p.ProductSpec AS [制剂规格],
	p.PackageSpec AS [包装规格],
	p.ProductStyle AS [药品类别],
	p.PhysicType AS [剂型],
	p.AuthorizedNo AS [批准文号],
	p.ProducerName AS [生产厂商],
	p.StockCondition AS [储存库区],
	bd.BatchNo AS [批号],
	bd.ProduceDate AS [生产日期],
	bd.ValidateDate AS [有效期至],
	b.BillCode AS [销售订单编号],
	b.BillDate AS [下单时间],
	b.BillTypeName AS [订单类型],
	b.Operator AS [业务员],
	b.DealerName AS [客户名称],
	b.DealerSpell AS [客户Spell],
	b.DealerAddress AS [客户地址],
	bd.UnitPrice AS [单价],
	bd.TradePrice AS [批发价],
	bd.RetailPrice AS [零售价],
	bd.Amount AS [数量],
	bd.TotalPrice AS [金额]
	FROM BUS_Bill b
		INNER JOIN BUS_BillDetail bd
			ON b.BillID = bd.BillID
		INNER JOIN BFI_Product p
			ON bd.ProductID = p.ProductID
	WHERE  p.ProductName  LIKE '%{TextProductName.Text}%' --药品名称
		 AND b.BillCode   LIKE '%{TextBillCode.Text}%'--[销售单编号]
		 AND bd.BatchNo   LIKE '%{TextBatchNo.Text}%'--[批号]
	   	 AND b.DealerName LIKE '%{TextDealerName.Text}%'--[供应商名称]
	 	 AND(b.BillDate   BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextBillDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextBillDatetwo.Value)}')
	     AND b.BillTypeName = '销售出货'--订单类型</SqlCommand>
      </销售记录>
