      <拒收记录 Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>拒收记录</Alias>
        <Columns isList="true" count="14">
          <value>拒收单号,System.String</value>
          <value>订单编号,System.String</value>
          <value>拒收日期,System.DateTime</value>
          <value>药品编号,System.String</value>
          <value>药品名称,System.String</value>
          <value>包装规格,System.String</value>
          <value>规格,System.String</value>
          <value>剂型,System.String</value>
          <value>拒收类型,System.String</value>
          <value>拒收原因,System.String</value>
          <value>收货数量,System.Int32</value>
          <value>拒收数量,System.Int32</value>
          <value>往来单位,System.String</value>
          <value>批号,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>拒收记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>select 
	d.DeclinedCode AS[拒收单号],
	d.BillCode AS[订单编号],
	d.DeclinedDate AS [拒收日期],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.PackageSpec AS [包装规格],
	p.ProductSpec AS [规格],
	p.PhysicType AS [剂型],
	d.DeclinedType AS[拒收类型],
	d.Reservation1 AS[拒收原因],
	de.Amount AS[收货数量],
	de.DeclinedAmount AS [拒收数量],
	d.DealerName AS [往来单位],
	de.BatchNo AS [批号]
	from BUS_Declined d
     	inner Join BUS_DeclinedDetail de
     		on d.DeclinedID=de.DeclinedID
     	inner JOIN BFI_Product p
     		on de.ProductID=p.ProductID

     	where d.DeclinedCode  LIKE '%{TextDeclinedCode.Text}%' --拒收单号
	 	 AND  d.BillCode      LIKE '%{TextBillCode.Text}%' --订单号
		 AND  de.BatchNo      LIKE '%{TextBatchNo.Text}%' --批号
		 AND  d.DeclinedType  LIKE '%{TextDeclinedType.Text}%'--[拒收类型]
		 AND  (d.DeclinedDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextDeclinedDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextDeclinedDatetwo.Value)}')</SqlCommand>
      </拒收记录>
