        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>数据源2</Name>
        <NameInSource>连接</NameInSource>
        <Parameters isList="true" count="1">
          <value>BillID,,8,0</value>
        </Parameters>
        <SqlCommand>

select 
	bu.BillDate 订单日期,
	bu.BillCode 订单编号,
	bu.DealerName 往来单位,
	bu.DealerAddress 送货地址,
	bu.TotalPrice 订单总额,
	bp.ProductName 产品名称,
	bp.ProducerName 生产厂商,
	bp.ProductSpec 规格型号,
	bp.ProductUnit 包装单位,
	bp.ProductStyle AS [产品类别],
	bp.AuthorizedNo 注册证号,
	bi.BatchNo 批号,
	bi.ProduceDate 生产日期,
	bi.ValidateDate 有效期至,
	bi.Amount 数量,
	bi.UnitPrice 单价,
	bi.TotalPrice 金额,
	convert(decimal(10,2),bi.Amount*1./bp.PackagingProportion) 件数,
	bu.Operator 制单人
	from BUS_Bill bu 
   left join BUS_BillDetail  bi
     on bu.BillID = bi.BillID
   left join BFI_Product bp 
	 on bp.ProductID = bi.productId
     
   where bu.BillTypeName = '销售退货' and bu.BillID = @BillID</SqlCommand>
      </数据源2>
      <大写金额 Ref="5" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>大写金额</Alias>
        <Columns isList="true" count="1">
          <value>总金额,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>大写金额</Name>
        <NameInSource>连接</NameInSource>
        <Parameters isList="true" count="1">
          <value>BillID,,8,0</value>
        </Parameters>
        <SqlCommand>SELECT 
	
	dbo.usf_ChineseCost(sum(bd.TotalPrice)) 总金额
	FROM dbo.BUS_BillDetail bd
	INNER JOIN dbo.BFI_Product p
		ON bd.ProductID = p.ProductID
WHERE bd.BillID = @BillID</SqlCommand>
      </大写金额>
