<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="DQS.App.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="DQS.App.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="DbConnectStrings" connectionString="Data Source=.; Initial Catalog=DQSHRYB;Integrated Security=false;UID=sa;PWD=bokesys.com"/>
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <userSettings>
    <DQS.App.Properties.Settings>
      <!-- 是否开启自动更新检测 -->
      <setting name="AutoCheckForUpdate" serializeAs="String">
        <value>True</value>
      </setting>
    </DQS.App.Properties.Settings>
  </userSettings>
  <applicationSettings>
    <DQS.App.Properties.Settings>
      <!-- 自动更新信息文件远程地址 -->
      <setting name="RemoteManifest" serializeAs="String">
        <value>http://localhost/BokesysSoft/DQS.manifest</value>
      </setting>
    </DQS.App.Properties.Settings>
  </applicationSettings>
</configuration>
