          <value>单位,System.String</value>
          <value>批号,System.String</value>
          <value>生产日期,System.DateTime</value>
          <value>有效期至,System.DateTime</value>
          <value>库存数量,System.Int32</value>
          <value>过期状态,System.String</value>
          <value>供货单位,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>今宵药品催销</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT 
	s.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.ProducerName AS [生产厂商],
	p.ProducerSpell AS [生产厂商Spell],
	p.ProductSpec AS [规格],
	p.PackageSpec AS [包装规格],
	p.PackagingProportion AS [包装比例],
	p.AuthorizedNo AS [批准文号],
	p.StockCondition AS [贮藏条件],
	p.PhysicType AS [剂型],
	p.ProductStyle AS [药品类别],
	p.ProductUnit AS [单位],
	s.BatchNo AS [批号],
	p.FirstDealerName [供货单位],
	s.ProduceDate AS [生产日期],
	s.ValidateDate AS [有效期至],
	s.Amount AS [库存数量]
				FROM BUS_StoreDetail s
				INNER JOIN BFI_Product p
	ON s.ProductID = p.ProductID AND s.Amount &gt; 0
	  WHERE  DATEADD(DAY,-p.BatchPreWarningDays,s.ValidateDate)&lt; GETDATE()
         AND (p.ProductCode    LIKE '%{TextProductName.Text}%' --药品编号
	 	 or p.ProductName     LIKE '%{TextProductName.Text}%') --药品名称
		 AND s.BatchNo        LIKE '%{TextBatchNo.Text}%'--[批号]
	   	</SqlCommand>
      </今宵药品催销>
