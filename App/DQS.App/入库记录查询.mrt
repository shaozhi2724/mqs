          <value>入库类型,System.String</value>
          <value>供应商名称,System.String</value>
          <value>入库人,System.String</value>
          <value>采购单号,System.String</value>
          <value>采购下单时间,System.DateTime</value>
          <value>采购员,System.String</value>
          <value>入库数量,System.Int32</value>
          <value>金额,System.Double</value>
          <value>单价,System.Double</value>
          <value>批号,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>入库记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT 
	p.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.CommonName AS [通用名称],
	p.ProductSpec AS [制剂规格],
	p.PackageSpec AS [包装规格],
	p.ProductStyle AS [药品类别],
	p.PhysicType AS [剂型],
	p.AuthorizedNo AS [批准文号],
	p.ProducerName AS [生产厂商],
	p.StockCondition AS [储存库区],
	sb.StoreCode AS [入库单号],
	sb.StoreDate AS [入库时间],
	sb.StoreTypeName AS [入库类型],
	sb.DealerName AS [供应商名称],
	sb.StoreKeeper AS [入库人],
	b.BillCode AS [采购单号],
	b.BillDate AS [采购下单时间],
	b.Operator AS [采购员],
	SUM(sbd.Amount*bb.UnitPrice) AS [金额],
	sbd.Amount AS [入库数量],
	bb.UnitPrice AS [单价],
	sbd.BatchNo AS [批号]
	FROM BUS_StoreBill sb
		INNER JOIN BUS_Bill b
			ON sb.PurchaseBillCode = b.BillCode
		INNER JOIN BUS_StoreBillDetail sbd
			ON	sb.StoreID = sbd.StoreID
		INNER JOIN BUS_BillDetail bb
			ON bb.ProductID = sbd.ProductID AND bb.BillID =b.BillID
		INNER JOIN BFI_Product p
			ON sbd.ProductID = p.ProductID
		WHERE    p.ProductName like '%{TextProductName.Text}%'--药品名
			AND   sb.StoreCode  like '%{TextStoreCode.Text}%'--入库单号
			AND   sbd.BatchNo  like '%{TextBatchNo.Text}%'--批号
			AND (sb.StoreDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextStoreDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextStoreDatetwo.Value)}')
    		AND   sb.StoreTypeName  like '%{TextStoreTypeName.Text}%'--入库类型

	GROUP BY
	p.ProductID,
	p.ProductCode,
	p.ProductName,
	p.ProductSpell,
	p.CommonName,
	p.ProductSpec,
	p.PackageSpec,
	p.ProductStyle,
	p.PhysicType,
	p.AuthorizedNo,
	p.ProducerName,
	p.StockCondition,
	sb.StoreCode,
	sb.StoreDate,
	sb.StoreTypeName,
	sb.DealerName,
	sb.StoreKeeper,
	b.BillCode,
	b.BillDate,
	b.Operator,
	b.TotalPrice,
	sbd.Amount,
	sbd.Amount,bb.UnitPrice,
	bb.UnitPrice,
	sbd.BatchNo</SqlCommand>
      </入库记录>
