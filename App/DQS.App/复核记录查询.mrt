          <value>业务员,System.String</value>
          <value>订单类型,System.String</value>
          <value>复核数量,System.Int32</value>
          <value>质量状况,System.String</value>
          <value>金额,System.Double</value>
          <value>单价,System.Double</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>复核记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT  
	p.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.CommonName AS [通用名称],
	p.ProductSpec AS [制剂规格],
	p.PackageSpec AS [包装规格],
	p.ProductStyle AS [药品类别],
	p.PhysicType AS [剂型],
	p.AuthorizedNo AS [批准文号],
	p.ProducerName AS [生产厂商],
	b.DealerName AS [客户名称],
	p.StockCondition AS [储存库区],
	rd.BatchNo AS [批号],
	rd.ProduceDate AS [生产日期],
	rd.ValidateDate AS [有效期至],
	r.ReviewCode AS [复核单号],
	r.ReviewDate AS [复核时间],
	r.ReviewPerson AS [复核人],
	b.BillCode AS [销售单号],
	b.BillDate AS [销售下单时间],
	b.Operator AS [业务员],
	b.BillTypeName AS [订单类型],
	rd.ReviewAmount AS [复核数量],
	b.TotalPrice AS [金额],
	bb.UnitPrice AS [单价],
	'合格' AS [质量状况]
	FROM BUS_Review r
		INNER JOIN BUS_ReviewDetail rd
			ON r.ReviewID = rd.ReviewID
		INNER JOIN BUS_Bill b
			ON r.SaleBillID = b.BillID
		INNER JOIN BFI_Product p
			ON rd.ProductID = p.ProductID
		INNER JOIN BUS_BillDetail bb
			ON bb.DetailID = rd.DetailID
					
	  WHERE  p.ProductName    LIKE '%{TextProductName.Text}%' --药品名称
		 AND rd.BatchNo       LIKE '%{TextBatchNo.Text}%'--[批号]
		 AND r.ReviewCode     LIKE '%{TextReviewCode.Text}%'--[ 复核单号]
		 AND (r.ReviewDate    BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextReviewDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextReviewDatetwo.Value)}')
         AND b.BillTypeName   LIKE '%{TextBillTypeName.Text}%'--出库类型
		 AND b.DealerName LIKE '%{TextDealerName.Text}%'
order by b.BillID</SqlCommand>
      </复核记录>
