          <value>批号,System.String</value>
          <value>复核单号,System.String</value>
          <value>单价,System.Double</value>
          <value>数量,System.Int32</value>
          <value>金额,System.Double</value>
          <value>验收日期,System.DateTime</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Product</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>

SELECT    
	p.ProductID AS 药品ID,
	p.ProductCode AS 药品编号, 
	p.ProductName AS 药品名称,
	p.ProductSpell AS [药品名称Spell], 
	p.CommonName AS 通用名称, 
	p.ProductSpec AS 规格, 
    p.PackageSpec AS 包装规格, 
	p.ProductStyle AS 药品类别, 
	p.PhysicType AS 剂型,
	p.AuthorizedNo AS 批准文号,
	p.ProducerName AS 生产厂商, 
	p.StockCondition AS 储存库区, 
    b.BillCode AS 采购订单编号,
	b.BillDate AS 退货日期,
	b.BillTypeName AS 订单类型,
	b.DealerName AS 退货单位,
	b.DealerSpell AS [客户Spell],
	b.SaleBillCode AS 销售单号,
	ba.AcceptPerson 验收人员,
	ba.AcceptResult 验收结果, 
    BillRemark 退货原因,
	ValidateDate 有效期,
	BatchNo 批号,
	ba.CreateDate [验收日期],
	b.ReviewCode AS 复核单号, 
	bd.UnitPrice AS 单价,
	bd.Amount AS 数量, 
	bd.TotalPrice AS 金额
	FROM	dbo.BUS_Bill AS b 
			INNER JOIN dbo.BUS_BillDetail AS bd ON b.BillID = bd.BillID 
			INNER JOIN dbo.BFI_Product AS p ON bd.ProductID = p.ProductID
            left join  bus_accept as  ba on ba.BillID = b.BillID 
					WHERE (b.IsBillIn = '1') AND (b.BillTypeID = '2')  
						and b.DealerName like'%{txtDealerName.Text}%' 
						AND (b.BillDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextBillDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextBillDatetwo.Value)}')
						and BatchNo like '%{txtBatchNo.Text}%'
	                    and p.ProductName like '%{txtProductName.Text}%'
					
							
					</SqlCommand>
      </Product>
