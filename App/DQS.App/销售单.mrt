          <value>小计金额,System.Double</value>
          <value>制单人,System.String</value>
          <value>件数,System.Decimal</value>
          <value>产品类别,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>数据源明细</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="1">
          <value>BillID,,8,0</value>
        </Parameters>
        <SqlCommand>SELECT 
	p.ProductName 产品名称,
	p.ProductSpec 规格型号 ,
	p.ProductUnit 包装单位,
	p.ProducerName 生产厂商,
	p.ProductStyle AS [产品类别],
	p.AuthorizedNo 注册证号,
	p.PackageSpec 包装规格型号,
	bd.BatchNo 批号,
	bd.ProduceDate 生产日期,
	bd.ValidateDate 有效日期,
	bd.Amount 数量,
	bd.UnitPrice 单价,
	bd.TotalPrice 小计金额,
	b.Operator 制单人,
	convert(decimal(10,2),bd.Amount*1./p.PackagingProportion) 件数
	FROM dbo.BUS_BillDetail bd
	INNER JOIN dbo.BFI_Product p
		ON bd.ProductID = p.ProductID
	INNER JOIN dbo.BUS_Bill b
	    ON bd.BillID = b.BillID
WHERE bd.BillID = @BillID</SqlCommand>
      </数据源明细>
