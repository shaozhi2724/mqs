          <value>生产厂商Spell,System.String</value>
          <value>创建人,System.String</value>
          <value>创建日期,System.DateTime</value>
          <value>最后修改人,System.String</value>
          <value>最后修改时间,System.DateTime</value>
          <value>备注,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Product</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>

SELECT     c.CuringID AS 养护ID, c.CuringCode AS 养护编号, c.CuringMan AS 养护人, c.CheckDate AS 养护日期, p.ProductCode AS 药品编号, p.ProductName AS 药品名称, p.ProductSpell AS 药品名称Spell, 
	c.BatchNo AS 药品批号, c.ProduceDate AS 生产日期, c.ValidateDate AS 有效期至, c.Amount AS 库存数量, c.QualifiedAmount AS 合格数量, c.UnqualifiedAmount AS 不合格数量, 
	c.CheckItem AS 检查项目, c.QualityCondition AS 质量状况, c.CuringMeasure AS 养护措施, c.CuringResult AS 处理结果, p.ProductSpec AS 规格, p.ProductUnit AS 单位, p.PackageSpec AS 包装规格, 
	p.PhysicType AS 剂型, p.ProductStyle AS 药品类别, p.AuthorizedNo AS 批准文号, p.ProducerName AS 生产厂商, p.ProducerSpell AS 生产厂商Spell, u1.UserName AS 创建人, 
	c.CreateDate AS 创建日期, u2.UserName AS 最后修改人, c.LastModifyDate AS 最后修改时间, c.CuringRemrk AS 备注
	FROM         dbo.BUS_CuringRecord AS c INNER JOIN
                      dbo.BFI_Product AS p ON c.ProductID = p.ProductID LEFT OUTER JOIN
                      dbo.ATC_User AS u1 ON c.CreateUserID = u1.UserID LEFT OUTER JOIN
                      dbo.ATC_User AS u2 ON c.LastModifyUserID = u2.UserID
		where c.CuringMan like '%{TxtPer.Text}%' 
		AND (c.CheckDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextCheckDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextCheckDatetwo.Value)}')
		and  c.BatchNo like '%{txtBatchNo.Text}%' 
		and  p.ProductName like '%{txtDealerName.Text}%'
  </SqlCommand>
      </Product>
