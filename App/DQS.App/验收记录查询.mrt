        <Dictionary isRef="1" />
        <Name>验收记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT  
	p.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.CommonName AS [通用名称],
	p.ProductSpec AS [规格],
	p.PackageSpec AS [包装规格],
	p.ProductStyle AS [药品类别],
	p.PhysicType AS [剂型],
	p.AuthorizedNo AS [批准文号],
	p.ProducerName AS [生产厂商],
	p.StockCondition AS [储存库区],
	ad.BatchNo AS [批号],
	ad.ProduceDate AS [生产日期],
	ad.ValidateDate AS [有效期至],
	a.AcceptCode AS [验收单号],
	a.AcceptDate AS [验收日期],
	a.AcceptPerson AS [验收人员姓名],
	b.BillCode AS [采购单号],
	b.BillDate AS [采购下单时间],
	b.BillTypeName AS [订单类型],
	b.DealerName AS [供货单位],
	b.Operator AS [采购人],
	ad.ReceiveAmount AS [到货数量],
	ad.QualifiedAmount AS [验收合格数量],
	ad.AcceptResult AS [验收结果],
	a.CreateDate AS [到货日期],
	ad.DetailRemark [不合格处理措施]
	FROM BUS_Accept a
		INNER JOIN BUS_AcceptDetail ad
			ON a.AcceptID = ad.AcceptID
		INNER JOIN BFI_Product p
			ON ad.ProductID = p.ProductID
		INNER JOIN BUS_Bill b
			ON a.BillID = b.BillID
       WHERE p.ProductName    LIKE '%{TextProductName.Text}%' --药品名称
		 AND a.AcceptCode     LIKE '%{TextAcceptCode.Text}%' --验收单号
	   	 AND ad.BatchNo       LIKE '%{TextaBatchNo.Text}%'--批号
	 	 AND (a.AcceptDate    BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextAcceptDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextAcceptDatetwo.Value)}')
		 AND b.BillTypeName  LIKE '%{TextBillTypeName.Text}%'--验收类型</SqlCommand>
      </验收记录>
