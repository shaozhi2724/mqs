      <Purchase Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>Purchase</Alias>
        <Columns isList="true" count="5">
          <value>StoreCode,System.String</value>
          <value>StoreDate,System.DateTime</value>
          <value>DealerName,System.String</value>
          <value>StoreTypeName,System.String</value>
          <value>TotalPrice,System.Double</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>Purchase</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT
	sb.StoreCode ,
	sb.StoreDate ,
	b.DealerName,
	sb.StoreTypeName,
	CASE b.BillTypeName
		WHEN '采购进货' THEN SUM(bd.UnitPrice*sbd.Amount)
		ELSE -SUM(bd.UnitPrice*sbd.Amount)
	END AS TotalPrice
FROM dbo.BUS_Bill b
	INNER JOIN dbo.BUS_StoreBill sb 
		ON ( b.BillID = sb.PurchaseBillID OR b.BillID = sb.SaleBillID )
	INNER JOIN dbo.BUS_BillDetail bd
		ON b.BillID = bd.BillID
	INNER JOIN dbo.BUS_StoreBillDetail sbd
		ON sb.StoreID = sbd.StoreID AND bd.ProductID = sbd.ProductID
WHERE b.BillTypeName IN ( '采购进货', '采购退货' )
	AND (sb.StoreDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",DTPCOne.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",DTPCTwo.Value)}')
	AND (b.DealerName LIKE '%{txtDealer.Text}%' OR b.DealerSpell LIKE '%{txtDealer.Text}%')
GROUP BY
	sb.StoreCode ,
	sb.StoreDate ,
	b.DealerName,
	sb.StoreTypeName,
	b.BillTypeName,sb.StoreID
ORDER BY {IIF(CbcOrder.Text=="单据号","sb.StoreID,b.DealerName","b.DealerName,sb.StoreID").ToString()}</SqlCommand>
      </Purchase>
