        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>收货记录</Name>
        <NameInSource>DQS</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT  
	p.ProductID AS [药品ID],
	p.ProductCode AS [药品编号],
	p.ProductName AS [药品名称],
	p.ProductSpell AS [药品名称Spell],
	p.CommonName AS [通用名称],
	p.ProductSpec AS [制剂规格],
	p.PackageSpec AS [包装规格],
	p.ProductStyle AS [药品类别],
	p.PhysicType AS [剂型],
	p.AuthorizedNo AS [批准文号],
	p.ProducerName AS [生产企业],
	p.StockCondition AS [储存库区],
	r.ReceiveCode AS [收货单号],
	r.ReceiveDate AS [到货时间],
	r.ReceivePerson AS [收货人员],
	r.BillTypeName AS [订单类型],
	r.DealerName AS [供应商名称],
	b.BillCode AS [采购单号],
	b.BillDate AS [采购下单时间],
	b.Operator AS [采购人],
	rd.Amount AS [数量],
	r.DealerName AS [发货单位],
	r.CarryCompnay AS [运输单位],
	r.Reservation3 AS [启运时间],
	r.TransportTool AS [运输工具],
	r.TransportCode AS [运输方式],
	bt.DealerAddress AS [发运地点],
	r.Reservation1 AS [到货温度]
	FROM BUS_Receive r
		INNER JOIN BUS_ReceiveDetail rd
			ON r.ReceiveID = rd.ReceiveID
		INNER JOIN BFI_Product p
			ON rd.ProductID = p.ProductID
		INNER JOIN BUS_Bill b
			ON r.BillID = b.BillID
		Left JOIN  BFI_Dealer bt 
			ON r.DealerID = bt.DealerID
	 WHERE  p.ProductName  LIKE '%{TextProductName.Text}%' 
		 AND r.ReceiveCode  LIKE '%{TextReceiveCode.Text}%'
		 AND (r.ReceiveDate BETWEEN '{Format("{0:yyyy-MM-dd 00:00:00}",TextReceiveDate.Value)}' AND '{Format("{0:yyyy-MM-dd 23:59:59}",TextReceiveDatetwo.Value)}')
     	 AND b.DealerName   LIKE '%{TextDealerName.Text}%'
		 AND r.BillTypeName Like '%{TextBillTypeName.Text}%'</SqlCommand>
      </收货记录>
